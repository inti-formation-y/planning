<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Cours.
    -->
    <changeSet id="20200319012000-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="cours_id"
                                 baseTableName="cours_eleve"
                                 constraintName="fk_cours_eleve_cours_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cours"/>

        <addForeignKeyConstraint baseColumnNames="eleve_id"
                                 baseTableName="cours_eleve"
                                 constraintName="fk_cours_eleve_eleve_id"
                                 referencedColumnNames="id"
                                 referencedTableName="eleve"/>
        
    </changeSet>
</databaseChangeLog>
